{
  "name": "bun-decompile",
  "version": "0.0.1",
  "description": "Extracts the original transpiled sources from an executable file generated via `bun build --compile`.",
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@luisafk/minifs": "^3.0.0",
    "@sgtpooki/file-type": "^1.0.1",
    "@sveltejs/adapter-static": "^3.0.4",
    "@sveltejs/kit": "^2.5.24",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@types/bun": "^1.1.8",
    "jszip": "^3.10.1",
    "lucide-svelte": "^0.436.0",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "sass": "^1.77.8",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.6",
    "svelte-highlight": "^7.7.0",
    "svgo": "^3.3.2",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.2"
  },
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "tests": "multibun run --from 0.6.0 -x ./src/scripts/dummyTests.sh -n --html static/tests.html",
    "format": "prettier --write .",
    "optimise-svg": "svgo -f static"
  },
  "trustedDependencies": [
    "@sveltejs/kit",
    "esbuild",
    "svelte-preprocess"
  ],
  "type": "module"
}
